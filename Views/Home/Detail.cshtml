@model List<CC_ASP.Models.ItemCountClass>

@{
    ViewData["Title"] = "Detail";
}

<h1>Detail</h1>

<div>
    <h4>@Model[0].ComputerName</h4>
    <hr />
    <dl class="row">
        @foreach(ItemCountClass i in @Model){
        <dt class = "col-sm-2">
            Item name
        </dt>
        <dd class = "col-sm-10">
            @i.name
        </dd>
        <dt class = "col-sm-2">
            Item count
        </dt>
        <dd class = "col-sm-10">
            @i.count
        </dd>
        }
    </dl>
</div>
<div>
    <a asp-action="Index">Back to List</a>
</div>
