@model List<StorageViewModel>
@{
    ViewData["Title"] = "Home Page";
}

<style>
    table, th, td {
        border: 1px solid black;
        border-collapse: initial;
    }
</style>

<table>
    <thead>
        <tr>
            <th>Název počítače</th>
            <th>Počet itemů</th>
            <th>Detail uložiště</th>
        </tr>
    </thead>
    <tbody>
        @foreach(StorageViewModel s in @Model){
        <tr>
            <td>@s.name</td>
            <td>@s.count</td>
            <td><a asp-controller="Home" asp-action="Detail" asp-route-ComputerName="@s.name">Details</a> </td>
        </tr>
        }
    </tbody>
</table>